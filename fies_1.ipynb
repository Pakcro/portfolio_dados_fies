    "# Entendendo a nota média e mediana do ENEM referente aos inscritos no FIES por ano\n",
    "import pandas as pd\n",
    "\n",
    "# Arquivo CSV\n",
    "insc = r'C:\\Users\\mathe\\OneDrive\\Documents\\portfólio\\fies\\inscritos_geral.csv'\n",
    "\n",
    "# Leitura do arquivo\n",
    "df_insc = pd.read_csv(insc, encoding='utf-8', delimiter=',', low_memory=False)\n",
    "\n",
    "# Convertendo a coluna 'Média nota Enem' para numérico e tratando erros (conversão de vírgulas para ponto)\n",
    "df_insc['Média nota Enem'] = pd.to_numeric(df_insc['Média nota Enem'].str.replace(',', '.'), errors='coerce')\n",
    "\n",
    "# Removendo linhas com valores NaN na coluna 'Média nota Enem' e 'Ano do processo seletivo' (caso existam)\n",
    "df_insc = df_insc.dropna(subset=['Média nota Enem', 'Ano do processo seletivo'])\n",
    "\n",
    "# Agrupando por ano e calculando as estatísticas\n",
    "estatisticas_por_ano = df_insc.groupby('Ano do processo seletivo')['Média nota Enem'].agg(\n",
    "    Média='mean',\n",
    "    Mediana='median',\n",
    "    Desvio_Padrão='std'\n",
    ").reset_index()\n",
    "\n",
    "# Formatando as colunas para duas casas decimais\n",
    "estatisticas_por_ano['Média'] = estatisticas_por_ano['Média'].round(2)\n",
    "estatisticas_por_ano['Mediana'] = estatisticas_por_ano['Mediana'].round(2)\n",
    "estatisticas_por_ano['Desvio_Padrão'] = estatisticas_por_ano['Desvio_Padrão'].round(2)\n",
    "\n",
    "# Resultados\n",
    "print(\"Estatísticas por ano: {estatisticas_por_ano:.}\")\n",
    "print(estatisticas_por_ano)\n"
   ]
